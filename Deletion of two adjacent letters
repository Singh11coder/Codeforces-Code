#include <bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin >> t;
    for(int i=0;i<n;i++) {
        string s, t;
        cin >> s >> t;
        bool yes = false;
        for(int i=0;i<s.length();i++)
            if (s[i] == t[0] && i % 2 == 0)
                yes = true;
        cout << (yes ? "YES" : "NO") << endl;
    }
}
