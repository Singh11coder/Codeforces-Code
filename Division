#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int r;
        cin>>r;
        if(r>=1900)
        {
            cout<<"Division 1"<<endl;
        }
        else if(r<=1899&&r>=1600)
        {
            cout<<"Division 2"<<endl;
        }
        else if(r<=1599&&r>=1400)
        {
            cout<<"Division 3"<<endl;
        }
        else
        {
            cout<<"Division 4"<<endl;
        }
    }
}
