#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n;
        cin>>n;
        string h;
        cin>>h;
        for(int i=0;i<h.length();i++)
        {
            if(h[i]=='U')
            {
                h[i]='D';
            }
            else if(h[i]=='D')
            {
                h[i]='U';
            }
        }
        
        cout<<h<<endl;
    }
}
