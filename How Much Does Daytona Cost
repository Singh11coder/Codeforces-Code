#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n,k;
        cin>>n>>k;
        vector<int>v(n);
        for(int i=0;i<v.size();i++)
        {
            cin>>v[i];
        }
        set<int>s;
        for(int i=0;i<v.size();i++)
        {
            s.insert(v[i]);
        }
        
        if(s.find(k)==s.end())
        {
            cout<<"NO"<<endl;
        }
        else
        {
            cout<<"YES"<<endl;
        }
    }
}
