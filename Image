#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        string a;
        string b;
        cin>>a;
        cin>>b;
        map<char,int>m;
        for(int i=0;i<a.length();i++)
        {
            m[a[i]]++;
        }
        for(int i=0;i<b.length();i++)
        {
            m[b[i]]++;
        }
        cout<<m.size()-1<<endl;
    }
}
