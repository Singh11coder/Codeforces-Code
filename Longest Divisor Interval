#include<bits/stdc++.h>
using namespace std;
int main(){
    long long n;
    cin>>n;
    while(n--){
        long long n1;
        cin>>n1;
        long long c=1;
       for(int i=1;i<=n1;i++){
        if(n1%i==0){
            c++;
        }
        else{
            break;
        }
       }
       c--;
       cout<<c<<endl;
    }
}
