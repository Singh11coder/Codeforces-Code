#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n=3;
        vector<int>v(n);
        for(int i=0;i<v.size();i++)
        {
            cin>>v[i];
        }
        sort(v.begin(),v.end());
        cout<<v[1]<<endl;
        
    }
}
