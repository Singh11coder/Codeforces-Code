#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n;
        cin>>n;
        vector<int>v;
        if(n&1)
        {
            
            for(int i=0;i<n;i++)
            {
                v.push_back(n);
            }
        }
        else
        {
            v.push_back(1);
            v.push_back(3);
            for(int i=2;i<n;i++)
            {
                v.push_back(2);
            }
        }
        
        for(int i=0;i<v.size();i++)
        {
            cout<<v[i]<<" ";
        }
        cout<<endl;
        
    }
}
