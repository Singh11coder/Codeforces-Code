#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n;
        cin>>n;
        vector<char>a;
        vector<char>b;
        char j;
        for(int i=0;i<n;i++)
        {
            cin>>j;
            a.push_back(j);
        }
        for(int i=0;i<n;i++)
        {
            cin>>j;
            b.push_back(j);
        }
        int flag=0;
        for(int i=0;i<n;i++)
        {
        if(a[i]!=b[i] && ((a[i]=='G' && b[i]=='B') || (a[i]=='B'&&b[i]=='G')))
            {
                   
            }
            else if(a[i]!=b[i])
            {
                    flag=1;
            }
        }
        if(flag==0)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
}
