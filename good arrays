#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n;
        cin>>n;
        int y;
        long long sum=0;
        int count=0;
        for(int i=0;i<n;i++)
        {
            cin>>y;
            if(y==1)count++;
            sum+=y;
        }
        

            if(sum>=(count+n) && n>1)
            {
                cout<<"YES"<<endl;
            }
            else
            {
                cout<<"NO"<<endl;
            }
        
        
    }
}
